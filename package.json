{
  "name": "arlula",
  "version": "1.0.0",
  "description": "JavaScript and TypeScript SDK for the Arlula satellite imagery API",
  "author": {
    "name": "Scott Owens",
    "email": "scott.owens.dev@gmail.com"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "TypeScript",
    "satellite",
    "satellite-images",
    "satellite-imagery",
    "eo",
    "gis",
    "geospatial"
  ],
  "homepage": "https://www.arlula.com",
  "bugs": "https://github.com/Arlula/jsSDK/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/Arlula/jsSDK.git"
  },
  "scripts": {
    "compile": "tsc",
    "clean": "rm -rf dist",
    "prebuildpackage": "npm run clean && npm run compile && node helpers/publishHelper.js",
    "buildpackage": "cd dist && npm pack",
    "test": "npx webpack --env tests && node ./e2e/dist/node.js && echo \"Open the browser test script (located at e2e/dist/browser.js) in a browser, errors will appear in the console\""
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.21",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
